{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "tsconfig.build.json",
    "assets": [
      {
        "include": "**/*.hbs",
        "outDir": "dist"
      }
    ]
  },
  "schematics": {
    "gateway": {
      "source": "my-nestjs-schematics",
      "factory": "gateway"
    }
  }
}